<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sugarsyntaxkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SugarSyntaxKotlin">
        <activity android:name=".DslActivity"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".reified.InlineWithReifiedExampleActivity"
            android:label="@string/title_activity_inline_with_reified_example"
            android:theme="@style/Theme.SugarSyntaxKotlin.NoActionBar" />

        <activity
            android:name=".sealed.SealedActivity"
            android:label="@string/title_activity_inline_with_reified_example"
            android:theme="@style/Theme.SugarSyntaxKotlin.NoActionBar" />
    </application>

</manifest>